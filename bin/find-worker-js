#!/bin/bash
# Order matters, so tootsville.js and game/start.js are pulled out.
# In the middle, all's fair, but for consistency, we alphabetize it.

echo worker/tootsville.js
find worker -name '*.js' \
  -and -not -name worker.js \
  -and -not -name '.#*' \
  -and -not -path worker/tootsville.js \
  -and -not -path worker/start.js | sort
find include -name '*.js' -and -not -name '.#*' | sort
echo worker/start.js
