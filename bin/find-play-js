#!/bin/bash
# Order matters, so tootsville.js and game/start.js are pulled out.
# In the middle, all's fair, but for consistency, we alphabetize it.

echo play/tootsville.js
find play -name '*.js' \
  -and -not -name '.#*' \
  -and -not -path 'play/lib/**' \
  -and -not -path 'play/ui/panels/**' \
  -and -not -path play/tootsville.js \
  -and -not -path play/game/start.js | sort
echo play/game/start.js
